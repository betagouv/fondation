{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint:check": "eslint .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "types:check": "tsc --noEmit -p ./tsconfig.app.json",
    "types:check:watch": "tsc --noEmit -p ./tsconfig.app.json --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:all": "vitest run && pnpm test:ct",
    "postinstall": "pnpm dsfr:build",
    "dsfr:build": "react-dsfr update-icons",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:watch": "PWTEST_WATCH=1 playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.17.0",
    "@reduxjs/toolkit": "^2.2.7",
