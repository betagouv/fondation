{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx",
    "format:check": "pnpm prettier --check src/**/*.{ts,tsx}",
    "types:check": "tsc --noEmit -p ./tsconfig.app.json",
