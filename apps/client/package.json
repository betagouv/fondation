{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:check": "eslint .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "types:check": "tsc --noEmit -p ./tsconfig.app.json",
    "types:check:watch": "tsc --noEmit -p ./tsconfig.app.json --watch",
    "test": "vitest",
    "test:watch": "vitest",
    "test:all": "vitest run",
    "postinstall": "pnpm dsfr:build",
    "dsfr:build": "react-dsfr update-icons",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:watch": "PWTEST_WATCH=1 playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.17.0",
    "@hookform/resolvers": "^5.0.1",
    "@sentry/react": "^9.14.0",
    "@sentry/tracing": "^7.120.3",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.2",
    "react-router-dom": "^7.6.2",
    "shared-models": "workspace:^",
    "zod": "^3.23.8",
    "@tiptap/extension-bold": "^2.11.5",
    "@tiptap/extension-bullet-list": "^2.11.5",
    "@tiptap/extension-color": "^2.11.3",
    "@tiptap/extension-document": "^2.11.5",
    "@tiptap/extension-heading": "^2.11.5",
    "@tiptap/extension-highlight": "^2.11.5",
    "@tiptap/extension-history": "^2.11.5",
    "@tiptap/extension-image": "^2.11.5",
    "@tiptap/extension-italic": "^2.11.5",
    "@tiptap/extension-list-item": "^2.11.3",
    "@tiptap/extension-ordered-list": "^2.11.5",
    "@tiptap/extension-paragraph": "^2.11.5",
    "@tiptap/extension-text": "^2.11.5",
    "@tiptap/extension-text-style": "^2.11.3",
    "@tiptap/extension-underline": "^2.11.5",
    "@tiptap/pm": "^2.11.3",
    "@tiptap/react": "^2.11.3",
    "@tiptap/starter-kit": "^2.11.3",
    "tiptap-extension-resize-image": "^1.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}