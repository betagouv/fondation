{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:check": "eslint .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "types:check": "tsc --noEmit -p ./tsconfig.app.json",
    "types:check:watch": "tsc --noEmit -p ./tsconfig.app.json --watch",
    "test": "vitest",
    "test:watch": "vitest",
    "test:all": "vitest run",
    "postinstall": "pnpm dsfr:build",
    "dsfr:build": "react-dsfr update-icons",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:watch": "PWTEST_WATCH=1 playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@codegouvfr/react-dsfr": "^1.17.0",
    "@hookform/resolvers": "^5.0.1",
    "@sentry/react": "^9.14.0",
    "@sentry/tracing": "^7.120.3",
    "clsx": "^2.1.1",
    "react-hook-form": "^7.56.2",
    "shared-models": "workspace:^",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}