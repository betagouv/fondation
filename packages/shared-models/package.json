{
  "name": "@/shared-models",
  "private": true,
  "version": "1.0.0",
  "description": "Shared models between the API and the Web App",
  "main": "./dist/cjs/models/index.js",
  "module": "./dist/esm/models/index.js",
  "types": "./dist/cjs/models/index.d.ts",
  "scripts": {
    "build:cjs": "rimraf dist/cjs && tsc -p tsconfig.build.cjs.json",
    "build:esm": "rimraf dist/esm && tsc -p tsconfig.build.esm.json",
    "build:cjs:watch": "tsc -p tsconfig.build.cjs.json --watch",
    "build:esm:watch": "tsc -p tsconfig.build.esm.json --watch"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist/cjs",
    "dist/esm"
  ]
}
